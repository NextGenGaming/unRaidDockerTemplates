<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Beta>False</Beta>
<Category>Network:Voip</Category>
  <Name>nggaming-jira</Name>
  <Description>
  </Description>
  <Overview>
 </Overview>
  <Support></Support>
  <Registry>https://hub.docker.com/r/cptactionhank/atlassian-jira-software/</Registry>
  <GitHub>https://github.com/NextGenGaming/unRaidDockerTemplates</GitHub>
  <Repository>cptactionhank/atlassian-jira-software</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
        <Port>
            <ContainerPort>8080</ContainerPort>
            <HostPort>8088</HostPort>
            <Protocol>tcp</Protocol>
        </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>JIRA_VERSION</Name>
      <Value>7.1.4</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>path to jira</HostDir>
      <ContainerDir>/var/atlassian/jira</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>path to logs</HostDir>
      <ContainerDir>/opt/atlassian/jira/logs</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:8088]/</WebUI>
  <Banner></Banner>
  <Icon></Icon>
  <ExtraParams></ExtraParams>
</Containers>