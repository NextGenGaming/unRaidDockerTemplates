<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Beta>False</Beta>
<Category>Network:Voip</Category>
  <Name>nggaming-stash</Name>
  <Description>
  </Description>
  <Overview>
 </Overview>
  <Support></Support>
  <Registry>https://bitbucket.org/atlassian/docker-atlassian-stash/</Registry>
  <GitHub>https://github.com/NextGenGaming/unRaidDockerTemplates</GitHub>
  <Repository>atlassian/docker-atlassian-stash</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
        <Port>
            <HostPort>7990</HostPort>
            <ContainerPort>7990</ContainerPort>
            <Protocol>tcp</Protocol>
        </Port>
        <Port>
            <HostPort>7999</HostPort>
            <ContainerPort>7999</ContainerPort>
            <Protocol>tcp</Protocol>
        </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>JIRA_VERSION</Name>
      <Value>7.1.4</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>path to stash</HostDir>
      <ContainerDir>/opt/atlassian/stash</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI></WebUI>
  <Banner></Banner>
  <Icon></Icon>
  <ExtraParams></ExtraParams>
</Containers>